    "$(CODEUTILS)/include"
    }
    
    if(_ACTION == "gmake") then
      buildoptions {"-std=c++0x"}
      pchheader ( "include/CommonPrecompiled.h" )
    else
      pchheader ( "CommonPrecompiled.h" )
    end
    pchsource ( "src/CommonPrecompiled.cpp" )

    flags { "Unicode", "NoIncrementalLink", "StaticRuntime"}
